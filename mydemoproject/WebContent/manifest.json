{
	"sap.app": {
		"id": "mydemoproject",
		"type": "application",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": false
		}
	},
	"sap.ui5": {
		"rootView": "mydemoproject.myviews.tileview",
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"mymodel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "data/mydata.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "mydemoproject.myviews",
				"controlId": ["App"],
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "firstview",
					"target": "firstview"
				},
				{
					"pattern": "mainview",
					"name": "mainview",
					"target": "mainview"
				},
				{
					"pattern": "cartview",
					"name": "mycart",
					"target": "cartview"
				},
				{
					"pattern": "paymentview",
					"name": "paymentview",
					"target": "paymentview"
				},
				{
				"pattern":"billdetails",
				"name":"mybilldetails",
				"target":"mybilldetails"
				},
				{
				"pattern":"Admin",
				"name":"myadmin",
				"target":"myadmin"
				}
			],
			"targets": {
				"firstview": {
					"viewName": "tileview",
					"viewType": "XML",
					"viewId": "view1"
				},
				"mainview": {
					"viewName": "mainview",
					"viewType": "XML",
					"viewId":"view2"
				},
				"cartview": {
					"viewName": "cart",
					"viewType": "XML",
					"viewId": "view3"
				},
				"paymentview": {
					"viewName": "payment",
					"viewType": "XML",
					"viewId": "view4"
				},
				"mybilldetails":{
				"viewName":"recieptview",
				"viewType":"XML"
				},
				"myadmin":{
				"viewName":"admin",
				"viewType":"XML"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		}
	}
}